{
	"name" : "pavlm/oauth2-tier",
	"homepage" : "https://github.com/pavlm/oauth2-tier",
	"description" : "OAuth2 proxy server based on amphp async framework.",
	"keywords" : [
		"oauth2",
		"proxy",
		"server",
		"php",
		"amphp",
		"async"
	],
	"authors" : [{
			"name" : "pavlm",
			"email" : "pavlm.dev@gmail.com"
		}
	],
	"require" : {
		"amphp/http-server" : "^3.3",
		"amphp/log" : "^2.0",
		"amphp/http-client" : "^5.1",
		"kelunik/oauth" : "dev-master",
		"symfony/dependency-injection" : "^6.4",
		"symfony/config" : "^6.4",
		"symfony/yaml" : "^6.4",
		"amphp/http-server-router" : "^2.0",
		"symfony/dotenv" : "^6.4",
		"amphp/byte-stream" : "^2.1",
		"amphp/http-server-session" : "^3.0",
		"symfony/expression-language" : "^6.4",
		"xantios/mimey": "v2.2.0"
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "./src"
		},
		"files" : [
			"src/functions.php"
		]
	},
	"scripts" : {
		"patch-vendor" : "bin/patch-vendor.sh"
	}
}
